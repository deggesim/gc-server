{
  "name": "gc",
  "version": "1.0.0",
  "description": "Gestione Casa",
  "author": "Simone De Gennaro",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deggesim/gc-server.git"
  },
  "bugs": {
    "url": "https://github.com/deggesim/gc-server/issues"
  },
  "homepage": "https://github.com/deggesim/gc-server#readme",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc --rootDir ./src/",
    "start": "ts-node src/index.ts",
    "local": "tsc --rootDir ./src/ & node dist/index.js",
    "start:dev": "env-cmd -f ./config/dev.env nodemon --config nodemon.json src/index.ts",
    "start:prod": "env-cmd -f ./config/prod.env nodemon --config nodemon.json src/index.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-jwt": "^4.0.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.0.3",
    "pg": "^8.5.1",
    "pg-connection-string": "^2.4.0",
    "typeorm": "^0.2.31",
    "typescript-ioc": "^3.2.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.13.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-jwt": "^3.3.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/lodash": "^4.14.168",
    "@types/luxon": "^3.0.1",
    "@types/node": "^14.14.32",
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.20",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  }
}
